// Expected output:
// > 1
// > 2
// > 3 fizz
// > 4
// > 5 buzz
// > 6 fizz
// > 7
// > 8
// > 9 fizz
// > 10 buzz
// > 11
// > 12 fizz
// > 13
// > 14
// > 15 fizz buzz
// > 16 not considered

let i = 1;
while i <= 15 do
begin
  print("~", i);
  if i % 3 == 0 then print(" fizz");
  if i % 5 == 0 then print(" buzz");
  print("\n");
  i <- i + 1;
end;

print("~ not considered\n", i);